logpath $logpath$/result_$timestamp$.log
delete $datapath$/persistit $datapath$/persistit_journal.*


clearprops
prop buffer.count.8192=8K
prop volume.1=$datapath$/persistit,create,bufferSize:8192,initialPages:10K,extensionPages:10K,maximumPages:1M
prop journalpath=$datapath$/persistit_journal
prop logfile=$logpath$/persistit_$timestamp$.log
prop rmiport=8081

#1 PersistitMapStress
init
test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000
test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 -t

test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000
test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 -t

test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 splay=17
test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 -t splay=17

test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 splay=179
test com.persistit.stress.PersistitMapStress1 repeat=1 count=1000000 -t splay=179

icheck persistit
waitIfFailed Please examine the failure!
close
