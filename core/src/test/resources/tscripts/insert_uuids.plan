logpath $logpath$/result_$timestamp$.log


#Stress-UUID single thread UUID insert - large pool

clearprops
prop buffer.count.8192=50000
prop volume.1=$datapath$/persistit,create,bufferSize:8192,initialSize:1G,extensionSize:1G,maximumSize:500G
prop journalpath=$datapath$/persistit_journal
prop logfile=$logpath$/persistit_$timestamp$.log
prop rmiport=8081

init
multithread

test com.persistit.stress.StressUUID repeat=100000 count=1000

icheck persistit
waitIfFailed Please examine the failure!
close


